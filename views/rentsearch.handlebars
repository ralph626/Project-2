<h1>Search Here for your new Home!</h1>
<form>
    <input id="cityName" type="text" placeholder="ENTER CITY NAME">
    <input id="address" type="text" placeholder="ENTER ADDRESS">
    <input id="beds" type="number" placeholder="NUMBER OF BEDROOMS" min="1" max="10">
    <input id="baths" type="number" placeholder="NUMBER OF BATHROOMS" min="1" max="10">
    <input id="propType" type="text" placeholder="PROPERTY TYPE">
    <input id="sqft" type="number" placeholder="SQUARE FOOTAGE">
    <input id="comps" type="number" placeholder="NUMBER OF COMPARABLE PROPERTIES" min="1">
    <button id="homeSearch">Search:</button>
</form>

